# PUBLIC INTERFACE - Environment configuration for Company Chatbot Frontend
# Copy this file to .env and fill values accordingly. Do not commit .env.

# Backend base URL (FastAPI), e.g., http://localhost:8000 or full deployed URL
REACT_APP_BACKEND_URL=http://localhost:8000

# Site URL used for auth email redirect (if applicable)
REACT_APP_SITE_URL=http://localhost:3000

# Optional: feature flags
REACT_APP_ENABLE_DEBUG=false
